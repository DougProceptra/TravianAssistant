{
  "name": "travian-assistant",
  "version": "1.0.0",
  "description": "Travian game assistant with Chrome extension",
  "main": "scripts/extract-all-kirilloid.js",
  "scripts": {
    "start": "node scripts/extract-all-kirilloid.js",
    "extract": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=true node scripts/extract-all-kirilloid.js",
    "extract-minimal": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=true node scripts/extract-playwright-final.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node backend/server.js",
    "build": "cd packages/extension && npm run build"
  },
  "keywords": ["travian", "game", "assistant", "chrome-extension"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "playwright": "^1.49.0",
    "puppeteer-core": "^21.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0"
  }
}
